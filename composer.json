{
    "name": "sbs/symfony-adminlte-bundle",
    "description": "Symfony AdminLTE Integration Bundle",
    "keywords": ["symfony", "adminlte"],
    "type": "symfony-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/koftikes/symfony-adminlte-bundle/issues",
        "source": "https://github.com/koftikes/symfony-adminlte-bundle/tree/1.5.x"
    },
    "authors": [
        {
            "name": "Konstantin Litvinov",
            "email": "koftikes@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "almasaeed2010/adminlte": "2.4.*",
        "bower-asset/jquery": ">=1.9 <4.0",
        "symfony/asset": "3.4.*",
        "symfony/console": "3.4.*",
        "symfony/event-dispatcher": "3.4.*",
        "symfony/framework-bundle": "3.4.*",
        "symfony/security-bundle": "3.4.*",
        "symfony/twig-bundle": "3.4.*",
        "symfony/yaml": "3.4.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "conflict": {
        "almasaeed2010/adminlte": "<2.4.4"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ],
    "autoload": {
        "psr-4": { "SbS\\AdminLTEBundle\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    }
}
